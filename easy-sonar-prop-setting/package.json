{
  "name": "easy-sonar-prop-setting",
  "displayName": "easy-sonar-prop-setting",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.95.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "easy-sonar-prop-setting.addSonarPath",
        "title": "Sonar Pathに追加"
      },
      {
        "command": "easy-sonar-prop-setting.addTestInclusion",
        "title": "Test Inclusionに追加"
      },
      {
        "command": "easy-sonar-prop-setting.addTestExclusion",
        "title": "Test Exclusionに追加"
      },
      {
        "command": "easy-sonar-prop-setting.clearAndAddSonarPath",
        "title": "Sonar Pathをクリアして追加"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "easy-sonar-prop-setting.clearAndAddSonarPath",
          "group": "easySonar@1"
        },
        {
          "command": "easy-sonar-prop-setting.addSonarPath",
          "group": "easySonar@2"
        },
        {
          "command": "easy-sonar-prop-setting.addTestInclusion",
          "group": "easySonar@3"
        },
        {
          "command": "easy-sonar-prop-setting.addTestExclusion",
          "group": "easySonar@4"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "npm run check-types && npm run lint && node esbuild.js",
    "watch": "npm-run-all -p watch:*",
    "watch:esbuild": "node esbuild.js --watch",
    "watch:tsc": "tsc --noEmit --watch --project tsconfig.json",
    "package": "npm run check-types && npm run lint && node esbuild.js --production",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "check-types": "tsc --noEmit",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.9",
    "@types/node": "20.x",
    "@types/vscode": "^1.95.0",
    "@typescript-eslint/eslint-plugin": "^8.10.0",
    "@typescript-eslint/parser": "^8.7.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "esbuild": "^0.24.0",
    "eslint": "^9.13.0",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.6.3"
  },
  "dependencies": {
    "dotenv": "^16.4.5"
  }
}
